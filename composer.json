{
    "name": "phpactor/worse-reference-finders",
    "description": "Worse Reflection reference finder implementations",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Leech",
            "email": "daniel@dantleech.com"
        }
    ],
    "require": {
        "phpactor/name-specification": "^0.1",
        "phpactor/reference-finder": "dev-maestro-php8",
        "phpactor/source-code-filesystem": "dev-maestro-php8",
        "phpactor/worse-reflection": "dev-maestro-php8",
        "php": "^7.3 || ^8.0"
    },
    "require-dev": {
        "ergebnis/composer-normalize": "^2.0",
        "friendsofphp/php-cs-fixer": "^2.17",
        "phpactor/test-utils": "dev-maestro-php8",
        "phpstan/phpstan": "~0.12.0",
        "phpunit/phpunit": "^9.0",
        "phpspec/prophecy-phpunit": "^2.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.2.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Phpactor\\WorseReferenceFinder\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Phpactor\\WorseReferenceFinder\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "integrate": [
            "./vendor/bin/php-cs-fixer fix",
            "./vendor/bin/phpstan analyse lib -c phpstan.neon",
            "./vendor/bin/phpunit"
        ]
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:Th3Mouk/glob"
        }
    ]
}
